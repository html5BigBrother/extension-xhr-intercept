{
  "name": "plasmo-demo",
  "displayName": "Plasmo demo",
  "version": "0.0.0",
  "description": "A basic Plasmo extension.",
  "author": "admin",
  "packageManager": "yarn@1.22.17",
  "scripts": {
    "dev": "plasmo dev",
    "dev:v2": "plasmo dev --target=chrome-mv2",
    "build": "plasmo build",
    "build:v2": "plasmo build --target=chrome-mv2"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.23.0",
    "plasmo": "^0.55.0",
    "pnpm": "^7.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "1.1.1",
    "@types/chrome": "0.0.193",
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  },
  "manifest": {
    "permissions": [
      "storage",
      "tabs"
    ],
    "host_permissions": [
      "https://*/*"
    ]
  }
}
